<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="501.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="campoUsuario" layoutX="210.0" layoutY="127.0" prefHeight="32.0" prefWidth="220.0" />
            <TextField fx:id="campoSenha" layoutX="210.0" layoutY="200.0" prefHeight="32.0" prefWidth="220.0" />
            <Label layoutX="290.0" layoutY="66.0" text="Login">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label layoutX="210.0" layoutY="109.0" text="Usuario" />
            <Label layoutX="210.0" layoutY="183.0" text="Senha" />
            <Button fx:id="botaoLogin" layoutX="281.0" layoutY="268.0" mnemonicParsing="false" onAction="#fazerLogin" prefHeight="25.0" prefWidth="78.0" text="Entrar" />
         </children>
    </AnchorPane>
  </children>
</VBox>
